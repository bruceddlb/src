    <script type="text/javascript">
        var contentPath = '@Url.Content("~")'.substr(0, '@Url.Content("~")'.length - 1);
        $(function () {
            //首页加载
            $.dmsindex.load();            
        });
    </script>
</head>
<body class="fixed-sidebar full-height-layout gray-bg" style="overflow:hidden">
    <div id="ajax-loader" style="cursor: progress; position: fixed; top: -50%; left: -50%; width: 200%; height: 200%; background: #fff; z-index: 10000; overflow: hidden;">
        <img src="~/Content/images/loading/ajax-loader.gif" style="position: absolute; top: 0; left: 0; right: 0; bottom: 0; margin: auto;" />
    </div>
    <div id="wrapper">
        <!--左侧导航开始-->
        <nav class="navbar-default navbar-static-side" role="navigation">
            <div class="nav-close">
                <i class="fa fa-times-circle"></i>
            </div>
            <div class="sidebar-collapse">
                <ul>
                    <li class="nav-header">
                        <div class="dropdown profile-element">
                            <span><img alt="image" class="img-circle" src="/Content/default/img/avatar.png" width="80" /></span>
                            <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                                <span class="clear">
                                    <span class="block m-t-xs"><strong class="font-bold">@QSDMS.Model.OperatorProvider.Provider.Current().Account</strong></span>
                                    <span class="text-muted text-xs block">@QSDMS.Model.OperatorProvider.Provider.Current().UserName<b class="caret"></b></span>
                                </span>
                            </a>
                            <ul class="dropdown-menu animated fadeInRight">
                                <li>
                                    <a class="J_menuItem" href="form_avatar.html">修改头像</a>
                                </li>
                                <li>
                                    <a class="J_menuItem" href="profile.html">个人资料</a>
                                </li>                                                              
                                <li class="divider"></li>
                                <li>
                                    <a href="#" onclick="$.dmsindex.indexOut()">安全退出</a>
                                </li>
                            </ul>
                        </div>
                        <div class="logo-element">
                            DMS
                        </div>
                    </li>
                </ul>
                <ul class="nav" id="side-menu"></ul>
            </div>
        </nav>
        <!--左侧导航结束-->
        <!--右侧部分开始-->
        <div id="page-wrapper" class="wrapper gray-bg" style="overflow:hidden">
            <div class="row border-bottom">
                <nav class="navbar navbar-static-top" role="navigation" style="margin-bottom: 0">
                    <div class="navbar-header">
                        <a class="navbar-minimalize minimalize-styl-2 btn btn-primary " href="#"><i class="fa fa-bars"></i> </a>
                    </div>
                    <ul class="nav navbar-top-links navbar-right">

                        <li class="dropdown">
                            <a class="dropdown-toggle count-info" data-toggle="dropdown" href="#">
                                <i class="fa fa-bell"></i> <span class="label label-primary">8</span>
                            </a>
                            <ul class="dropdown-menu dropdown-alerts">
                                <li>
                                    <a href="mailbox.html">
                                        <div>
                                            <i class="fa fa-envelope fa-fw"></i> 您有16条未读消息
                                            <span class="pull-right text-muted small">4分钟前</span>
                                        </div>
                                    </a>
                                </li>
                                <li class="divider"></li>
                                <li>
                                    <a href="profile.html">
                                        <div>
                                            <i class="fa fa-qq fa-fw"></i> 3条新回复
                                            <span class="pull-right text-muted small">12分钟钱</span>
                                        </div>
                                    </a>
                                </li>
                                <li class="divider"></li>
                                <li>
                                    <div class="text-center link-block">
                                        <a class="J_menuItem" href="notifications.html">
                                            <strong>查看所有 </strong>
                                            <i class="fa fa-angle-right"></i>
                                        </a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                        <li style="cursor:pointer" onclick="$.dmsindex.indexOut()"><a><i class="fa fa fa-sign-out"></i> 退出</a></li>
                    </ul>
                </nav>
            </div>
            <!--中间内容-->
            <div id="content-wrapper" class="content-wrapper">
                <div class="page-content">
                    <!--导航菜单tab-begin-->
                    <div class="row content-tabs">
                        <button class="roll-nav roll-left tabLeft" onclick='$.dmsindex.scrollTab(false);'>
                            <i class="fa fa-backward"></i>
                        </button>
                        <nav class="page-tabs menuTabs">
                            <div id="tab_list_add" class="page-tabs-content">
                            </div>
                        </nav>
                        <div class="contextmenu">
                            <ul>
                                <li onclick="$.dmsindex.removeTab('reloadCurrent')">刷新当前</li>
                                <li onclick="$.dmsindex.removeTab('closeCurrent')">关闭当前</li>
                                <li onclick="$.dmsindex.removeTab('closeAll')">全部关闭</li>
                                <li onclick="$.dmsindex.removeTab('closeOther')">除此之外全部关闭</li>
                                <li class='m-split'></li>
                                <li onclick="$.dmsindex.indexOut()">退出</li>
                            </ul>
                        </div>
                        <button class="roll-nav roll-right tabRight" style="margin-right:40px;" onclick='$.dmsindex.scrollTab(true);'>
